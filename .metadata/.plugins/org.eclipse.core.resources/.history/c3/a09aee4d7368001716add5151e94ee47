package cn.yesway.pms.core.cast.resource.dao;

import java.util.Map;

import cn.yesway.pms.common.core.dao.BaseDaoImpl;
import cn.yesway.pms.common.page.PageBean;
import cn.yesway.pms.common.page.PageParam;
import cn.yesway.pms.core.cast.resource.entity.ResourceCastReport;

public class ResourceCastReportDaoImpl extends BaseDaoImpl<ResourceCastReport> implements ResourceCastReportDao {

	@Override
	public PageBean<ResourceCastReport> reviewListByPageAndParam(PageParam page, Map<String, Object> map) {
		String sqlId = super.getStatement("reviewListByPageAndParam");
		return super.listByPageAndParam(page, map, sqlId);
	}
	
}
