package cn.yesway.pms.core.cast.resource.entity;

import cn.yesway.pms.core.project.entity.Project;

public class ResourceAmountProjectDetail {
	private Project project;
	private Integer scala;

	public Project getProject() {
		return project;
	}

	public void setProject(Project project) {
		this.project = project;
	}

	public Integer getScala() {
		return scala;
	}

	public void setScala(Integer scala) {
		this.scala = scala;
	}

}
