package cn.yesway.pms.core.cast.resource.entity;

import java.math.BigDecimal;
import java.util.Date;


public class ResourceCastReport {
	
	private BigDecimal pre_tax_cast;
	private BigDecimal tas_money;
	private BigDecimal after_tax_cast;
	private Date affiliationDate;

	public BigDecimal getPre_tax_cast() {
		return pre_tax_cast;
	}

	public void setPre_tax_cast(BigDecimal pre_tax_cast) {
		this.pre_tax_cast = pre_tax_cast;
	}

	public BigDecimal getTas_money() {
		return tas_money;
	}

	public void setTas_money(BigDecimal tas_money) {
		this.tas_money = tas_money;
	}

	public BigDecimal getAfter_tax_cast() {
		return after_tax_cast;
	}

	public void setAfter_tax_cast(BigDecimal after_tax_cast) {
		this.after_tax_cast = after_tax_cast;
	}

	public Date getAffiliationDate() {
		return affiliationDate;
	}

	public void setAffiliationDate(Date affiliationDate) {
		this.affiliationDate = affiliationDate;
	}

}
